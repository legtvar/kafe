{
  "workbench.colorCustomizations": {
    "activityBar.background": "#483f39",
    "titleBar.activeBackground": "#362d26",
    "titleBar.activeForeground": "#FFF8E7" // cosmic latte
  },
  "deno.enablePaths": [
    "./Docs"
  ],
  "deno.disablePaths": [
    "./Web"
  ],
  "i18n-ally.localesPaths": "Web/translations",
  "i18n-ally.enabledFrameworks": [
    "react",
    "react-i18next",
    "general",
    "i18next",
    "vscode"
  ],
  "typescript.tsdk": "Web/node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
}
